/**/int analogPin = 0;     // Sense terminal at skullint switchPin = 2;                       // outside leads to ground and +5Vint val = 0;           // variable to store the value readvoid setup() {    Serial.begin(9600);          //  setup serial  pinMode(switchPin, OUTPUT);           // set pin to input  digitalWrite(switchPin, LOW);}void loop() {  val = analogRead(analogPin);    // read the input pin  if(val>800)  {    digitalWrite(switchPin, HIGH);  }  else  {    digitalWrite(switchPin, LOW);  }  Serial.print(val);  Serial.print('\n');}